    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'backseat_safe_system_2k'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=backseat_safe_system_2k",
                    "--package=backseat_safe_system_2k"
                ],
                "filter": {
                    "name": "backseat_safe_system_2k",
                    "kind": "bin"
                }
            },
            "args": [
                "./roms/rom.backseat"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Release executable 'backseat_safe_system_2k'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=backseat_safe_system_2k",
                    "--package=backseat_safe_system_2k",
                    "release"
                ],
                "filter": {
                    "name": "backseat_safe_system_2k",
                    "kind": "bin"
                }
            },
            "args": [
                "./roms/rom.backseat"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'backseat_safe_system_2k'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=backseat_safe_system_2k",
                    "--package=backseat_safe_system_2k"
                ],
                "filter": {
                    "name": "backseat_safe_system_2k",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}